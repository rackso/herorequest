{
  "name": "@schibstedspain/re-card",
  "version": "13.0.1",
  "main": "lib/",
  "scripts": {
    "clean:dist": "rimraf ./dist/*",
    "clean:lib": "rimraf ./lib/*",
    "predist": "npm run clean:dist -s",
    "dist": "webpack",
    "predoc": "rimraf ./docs/dist/*",
    "doc": "npm run dist && npm run doc:copy && npm run doc:publish",
    "doc:copy": "copyfiles ./dist/* ./docs/",
    "doc:publish": "gh-pages -d docs",
    "prelib": "npm run clean:lib -s",
    "lib": "npm run lib:scripts && npm run lib:styles",
    "lib:scripts": "babel src --out-dir lib",
    "lib:styles": "copyfiles -u 1 './src/**/*.scss' lib",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:eslint": "eslint --ext=.js --ext=.jsx ./src/ ./test/ ./docs/",
    "lint:sass": "node_modules/.bin/sass-lint src/**/*.scss -c -v",
    "phoenix": "rimraf ./node_modules/ && npm install",
    "prepublish": "NODE_ENV=production npm run lib && npm run dist",
    "start": "npm run start:open && npm run start:server",
    "start:server": "webpack-dev-server",
    "start:open": "opener http://localhost:8080",
    "test": "NODE_ENV=test mocha --compilers js:babel-register --recursive --require babel-polyfill",
    "test:watch": "npm run test -- -w test src --watch-extensions jsx",
    "lint:js": "node_modules/.bin/eslint --ext .js,.jsx ./"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.schibsted.io:scmspain/frontend-fc--uilib-card.git"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "peerDependencies": {
    "react": "0.14"
  },
  "dependencies": {
    "@schibstedspain/fotocasa-iconset": "1.0",
    "@schibstedspain/re-contact": "1.0",
    "@schibstedspain/real-estate": "4",
    "@schibstedspain/rosetta": "3.10",
    "@schibstedspain/sui-card": "7.2",
    "@schibstedspain/sui-label": "0.1",
    "@schibstedspain/sui-modal": "1.3",
    "@schibstedspain/sui-multimedia": "0.4",
    "@schibstedspain/theme-basic": "6",
    "classnames": "2.2.0",
    "react-slidy": "1.1"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-core": "6.7.2",
    "babel-loader": "6.2.4",
    "babel-preset-schibsted-spain": "2",
    "chai": "3.5.0",
    "copyfiles": "0.2.1",
    "css-loader": "0.14.5",
    "extract-text-webpack-plugin": "1.0.1",
    "gh-pages": "0.11.0",
    "jsdom": "7.0.2",
    "lodash": "3.9.3",
    "mocha": "2.4.5",
    "node-libs-browser": "0.5.2",
    "node-sass": "3.4.2",
    "opener": "1.4.1",
    "react": "0.14",
    "react-addons-test-utils": "0.14.6",
    "react-dom": "0.14",
    "react-hot-loader": "1.2.7",
    "react-shallow-testutils": "0.6.0",
    "rimraf": "2.5.0",
    "sass-loader": "3.1.2",
    "sinon": "1.17.2",
    "style-loader": "0.12.3",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1",
    "webpack-merge": "0.8.3"
  }
}
